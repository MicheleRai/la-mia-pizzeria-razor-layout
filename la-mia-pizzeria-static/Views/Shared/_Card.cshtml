<div class="col">
    <div class="card" style="width: 30rem">
        <img src=@Model.Foto class="card-img-top " alt="foto">
        <div class="card-body">
            <h5 class="card-title text-center">@Model.Name</h5>
            <p class="card-text">@Model.Description</p>
            <p class="fw-bold">@Model.Prezzo €</p>
        </div>
    </div>
</div>
